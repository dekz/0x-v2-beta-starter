{
    "name": "0x-v2-beta-starter-project",
    "version": "0.0.1",
    "main": "lib/scenarios/fill_order.js",
    "engines": {
        "node": ">=6.12"
    },
    "description": "0x v2 beta",
    "scripts": {
        "lint": "tslint --project . --exclude **/src/contract_wrappers/**/*",
        "watch_without_deps": "cross-env yarn pre_build && tsc -w",
        "build": "cross-env yarn pre_build && tsc",
        "pre_build": "run-s generate_contract_wrappers copy_artifacts",
        "clean": "shx rm -rf lib artifacts src/contract_wrappers",
        "copy_artifacts": "copyfiles -u 1 './src/artifacts/**/*' ./lib",
        "download_snapshot": "cross-env download --extract --strip 1 --out 0x_testrpc_snapshot ${npm_package_config_snapshot_url}",
        "ganache-cli": "cross-env ganache-cli --db 0x_testrpc_snapshot --noVMErrorsOnRPCResponse -p 8545 --networkId 50 -m \"${npm_package_config_mnemonic}\"",
        "testrpc": "yarn ganache-cli",
        "scenario:all": "cross-env yarn run build && node ./lib/scenarios/all.js",
        "scenario:fill_order": "cross-env yarn run build && node ./lib/scenarios/fill_order.js",
        "scenario:fill_order_fees": "cross-env yarn run build && node ./lib/scenarios/fill_order_fees.js",
        "scenario:fill_order_erc721": "cross-env yarn run build && node ./lib/scenarios/fill_order_erc721.js",
        "scenario:match_orders": "cross-env yarn run build && node ./lib/scenarios/match_orders.js",
        "scenario:execute_transaction": "cross-env yarn run build && node ./lib/scenarios/execute_transaction.js",
        "generate_contract_wrappers": "cross-env abi-gen --abis ${npm_package_config_abis} --template ./contract_templates/contract.handlebars --partials './contract_templates/partials/**/*.handlebars' --output src/contract_wrappers --backend ethers"
    },
    "config": {
        "abis": "./src/artifacts/@(DummyERC20Token|DummyERC721Token|ERC20Proxy|ERC721Proxy|Exchange|WETH9|ZRXToken).json",
        "mnemonic": "concert load couple harbor equip island argue ramp clarify fence smart topic",
        "snapshot_url": "https://s3.amazonaws.com/testrpc-shapshots/a83f5241dc84984a5db46dd14955e1fa052aad59.zip"
    },
    "author": "",
    "license": "Apache-2.0",
    "dependencies": {
        "@0xproject/abi-gen": "^0.3.2",
        "@0xproject/base-contract": "^0.3.4",
        "@0xproject/order-utils": "^0.0.7",
        "@0xproject/sol-compiler": "^0.5.2",
        "@0xproject/subproviders": "^0.10.4",
        "@0xproject/tslint-config": "^0.4.20",
        "@0xproject/types": "^0.8.1",
        "@0xproject/typescript-typings": "^0.4.1",
        "@0xproject/utils": "^0.7.1",
        "@0xproject/web3-wrapper": "^0.7.1",
        "@types/mocha": "^5.2.2",
        "cli-table": "^0.3.1",
        "copyfiles": "^2.0.0",
        "ethereum-types": "^0.0.2",
        "ethers": "3.0.22",
        "lodash": "^4.17.4",
        "ora": "^2.1.0",
        "run-s": "^0.0.0",
        "web3-provider-engine": "^14.0.4"
    },
    "devDependencies": {
        "@0xproject/dev-utils": "^0.4.4",
        "chai": "^4.0.1",
        "chai-as-promised": "^7.1.0",
        "chai-bignumber": "^2.0.1",
        "cross-env": "^5.1.4",
        "dirty-chai": "^2.0.1",
        "download-cli": "^1.0.5",
        "ganache-cli": "^6.1.4",
        "make-promises-safe": "^1.1.0",
        "npm-run-all": "^4.1.2",
        "shx": "^0.2.2",
        "tslint": "5.8.0",
        "typescript": "2.7.1"
    }
}
